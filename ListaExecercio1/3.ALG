algoritmo "helloworld"
var
   a, b, c, p, area: real
inicio
   escreva("insira o valor do lado a: ")
   leia(a)
   escreva("insira o valor do lado b: ")
   leia(b)
   escreva("insira o valor do lado c: ")
   leia(c)
   p <- (a + b + c) / 2
   area <- RaizQ(p * (P - a) * (P - b) * (P - c))
   escreva("O valor da area e igual a: ", area)
fimalgoritmo