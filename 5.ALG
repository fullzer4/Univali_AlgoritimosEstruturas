algoritmo "helloworld"
var
   numero, dobro, cubo, rquadrado, metade: real
inicio
   escreva("Insira seu numero: ")
   leia(numero)
   dobro <- numero * 2
   cubo <- numero ^ 3
   rquadrado <- RaizQ(numero)
   metade <- numero / 2
   escreva("O dobro e: ", dobro, " e o cubo: ", cubo, " e a raiz e: ", rquadrado, "e a metade e: ", metade)
fimalgoritmo