algoritmo "helloworld"
var
   largura, altura, mquadrado, tinta: real
inicio
   escreva("insira o valor altura: ")
   leia(altura)
   escreva("insira o valor largura: ")
   leia(largura)
   mquadrado <- altura * largura
   tinta <- mquadrado / 2
   escreva("Precisa de: ", tinta, " litros de tinta")

fimalgoritmo