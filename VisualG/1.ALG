algoritmo "helloworld"
var
   pessoa_um, pessoa_dois: caracter
inicio
   escreva("insira o nome da 1 pessoa: ")
   leia(pessoa_um)
   escreva("")
   escreva("insira o nome da 2 pessoa: ")
   leia(pessoa_dois)
   escreva("")
   escreva("Bem vindo ", pessoa_um, " e ", pessoa_dois)
fimalgoritmo