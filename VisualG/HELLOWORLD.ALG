algoritmo "helloworld"
var
   numero_um, numero_dois, resultado: inteiro
inicio
   escreva("insira o 1 numero: ")
   leia(numero_um)
   escreva("")
   escreva("insira o 2 numero: ")
   leia(numero_dois)
   escreva("")
   resultado = numero_um + numero_dois
   escreva("O produto dos dois e: ", resultado)
fimalgoritmo