algoritmo "helloworld"
var
   notaum, notadois, resultado: real
inicio
   escreva("insira a nota 1: ")
   leia(notaum)
   escreva("")
   escreva("insira a nota 2: ")
   leia(notadois)
   escreva("")
   resultado <- (notaum + notadois) / 2.0
   escreva("A media e: ", resultado)
fimalgoritmo