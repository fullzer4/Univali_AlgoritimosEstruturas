algoritmo "helloworld"
var
   numeroum, numerodois, resultado: inteiro
inicio
   escreva("insira o 1 numero: ")
   leia(numeroum)
   escreva("")
   escreva("insira o 2 numero: ")
   leia(numerodois)
   escreva("")
   resultado <- numeroum + numerodois
   escreva("O produto dos dois e: ", resultado)
fimalgoritmo